<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
	<joined-subclass name="com.kidscademy.atlas.Instrument" extends="com.kidscademy.atlas.AtlasObject" table="instrument">
		<key column="id" />
		<property name="category" type="com.kidscademy.dao.InstrumentCategoryHbm" />
		<property name="sampleTitle" />
		<property name="samplePath" />
	</joined-subclass>
</hibernate-mapping>
