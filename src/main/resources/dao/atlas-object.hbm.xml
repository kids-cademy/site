<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
	<class name="com.kidscademy.atlas.AtlasObject" table="atlas_object">
		<id name="id">
			<generator class="native" />
		</id>

		<property name="index" column='"index"' />
		<property name="rank" />
		<property name="name" />
		<property name="display" />
		<property name="description" />
		<property name="iconPath" />
		<property name="thumbnailPath" />
		<property name="picturePath" />

		<set name="links" inverse="false" lazy="false" cascade="all" fetch="select" table="object_link">
			<key column="objectId" />
			<many-to-many class="com.kidscademy.atlas.Link" column="linkId" />
		</set>

		<!-- <map name="facts"> <key column="name" /> <map-key column="name" /> <one-to-many class="com.kidscademy.atlas.Fact" /> </map> -->
	</class>
</hibernate-mapping>
