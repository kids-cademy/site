<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
	<class name="com.kidscademy.model.CrashReport" table="crash_report">
		<id name="id">
			<generator class="native" />
		</id>
		<many-to-one name="app" column="appId" class="com.kidscademy.model.App" lazy="false" /> 
		<property name="timestamp" />
		<property name="ip" />
		<many-to-one name="device" column="deviceId" class="com.kidscademy.model.Device" lazy="false" /> 
		<property name="stackTrace" />
	</class>
</hibernate-mapping>
